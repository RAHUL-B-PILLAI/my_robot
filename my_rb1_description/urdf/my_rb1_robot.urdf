<robot name="rb1 robot">

	<link name="base_link"/>
	<link name="body">
		<inertial>
			<mass value="25" />
			<origin xyz="0 0 0" />
			<inertia ixx="0.578125" ixy="0" ixz="0" iyy="0.578125" iyz="0" izz="0.78125" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.25" length="0.3" />
			</geometry>
			<material name="blue">
				<color rgba="0 0 1 1" />
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.25" length="0.3" />
			</geometry>
		</collision>
	</link>
	<link name="base_footprint" />
	<link name="left_wheel">
		<!-- Define the visual and collision properties of the left wheel -->
		<visual>
			<origin xyz="0 0 0" rpy="1.5 0 0" />
			<geometry>
				<cylinder radius="0.025" length="0.05" />
			</geometry>
			<material name="green">
				<color rgba="0 1 0 1" />
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="1.5 0 0" />
			<geometry>
				<cylinder radius="0.025" length="0.05" />
			</geometry>
		</collision>
		<inertial>
			<mass value="1" />
			<origin xyz="0 0 0" />
			<inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
		</inertial>
	</link>
	<link name="right_wheel">
		<!-- Define the visual and collision properties of the right wheel -->
		<visual>
			<origin xyz="0 0 0" rpy="1.5 0 0" />
			<geometry>
				<cylinder radius="0.025" length="0.05" />
			</geometry>
			<material name="green">
				<color rgba="0 1 0 1" />
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="1.5 0 0" />
			<geometry>
				<cylinder radius="0.025" length="0.05" />
			</geometry>
		</collision>
		<inertial>
			<mass value="1" />
			<origin xyz="0 0 0" />
			<inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
		</inertial>
	</link>
	<!-- Define the link for the front caster wheel -->
	<link name="front_caster_wheel">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<sphere radius="0.025" />
			</geometry>
			<material name="black">
				<color rgba="0 0 0 1" />
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<sphere radius="0.025" />
			</geometry>
		</collision>
		<!-- Define the inertial properties for the front caster wheel -->
		<inertial>
			<mass value="1" />
			<origin xyz="0 0 0" />
			<inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001" />
		</inertial>
	</link>
	<!-- Define the link for the back caster wheel -->
	<link name="back_caster_wheel">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<sphere radius="0.025" />
			</geometry>
			<material name="black">
				<color rgba="0 0 0 1" />
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<sphere radius="0.025" />
			</geometry>
		</collision>
		<!-- Define the inertial properties for the back caster wheel -->
		<inertial>
			<mass value="1" />
			<origin xyz="0 0 0" />
			<inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001" />
		</inertial>
	</link>
	<!-- Define the link for the front laser -->
	<link name="front_laser">
		<visual>
			<origin xyz="0 0 0" />
			<!-- 180ยบ rotation in x-axis -->
			<geometry>
				<box size="0.05 0.05 0.1" />
			</geometry>
			<material name="red">
				<color rgba="1 0 0 1" />
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0" />
			<geometry>
				<box size="0.05 0.05 0.1" />
			</geometry>
		</collision>
		<!-- Define the inertial properties for the front laser -->
		<inertial>
			<mass value="1" />
			<!-- Negligible mass for the laser -->
			<origin xyz="0 0 0" rpy="0 0 0" />
			<inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
		</inertial>
	</link>
	<!-- Define the joint to connect the base_footprint to the base -->
	<joint name="body_joint" type="fixed">
		<parent link="base_link" />
		<child link="body" />
		<origin xyz="0 0 0" />
	</joint>
	<!-- Define the joint to connect the base_footprint to the base -->
	<joint name="base_footprint_joint" type="fixed">
		<parent link="base_link" />
		<child link="base_footprint" />
		<origin xyz="0 0 -0.15" />
	</joint>
	<!-- Define the joint to connect the left wheel to the base -->
	<joint name="left_wheel_joint" type="continuous">
		<parent link="base_link" />
		<child link="left_wheel" />
		<origin rpy="0 0 0" xyz="0 -0.2 -0.15" />
		<axis rpy="0 0 0" xyz="0 1 0" />
		<limit effort="10000" velocity="1000" />
	</joint>
	<!-- Define the joint to connect the right wheel to the base -->
	<joint name="right_wheel_joint" type="continuous">
		<parent link="base_link" />
		<child link="right_wheel" />
		<origin rpy="0 0 0" xyz="0 0.2 -0.15" />
		<axis rpy="0 0 0" xyz="0 1 0" />
		<limit effort="10000" velocity="1000" />
	</joint>
	<!-- Define the joint to connect the front caster wheel to the base -->
	<joint name="front_caster_joint" type="fixed">
		<parent link="base_link" />
		<child link="front_caster_wheel" />
		<origin xyz="-0.1 0 -0.15" />
		<!-- Align the caster wheel with its position -->
	</joint>
	<!-- Define the joint to connect the back caster wheel to the base -->
	<joint name="back_caster_joint" type="fixed">
		<parent link="base_link" />
		<child link="back_caster_wheel" />
		<origin xyz="0.1 0 -0.15" />
		<!-- Align the caster wheel with its position -->
	</joint>
	<!-- Define the joint to connect the laser to the base -->
	<joint name="front_laser_joint" type="fixed">
		<parent link="base_link" />
		<child link="front_laser" />
		<origin xyz="0.25 0.0 0.075" rpy="3.14159 0 0" />
	</joint>


</robot>